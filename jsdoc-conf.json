{
  "opts": {
    "lenient": true,
    "destination": "./www/docs/",
    "recurse": true
  },
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": "./www/app",
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"]
}
